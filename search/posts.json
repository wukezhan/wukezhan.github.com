---
layout: nil
---
[{% for post in site.posts %}
	{
		"title": "{{ post.title|replace:'"', '\\"' }}",
		"date": "{{ post.date|date:'%Y-%m-%d %H:%i:%s' }}",
		"cates": ["{{ post.categories|join:'", "' }}"],
		"tags": ["{{ post.categories|join:'", "' }}"],
	}{% if forloop.last %},{% endif %}
{% endfor %}]